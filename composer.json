{
    "name": "ripaclub/sphinxsearch",
    "description": "A Zend Framework 2 module that encapsulates Sphinx Search functionalities.",
    "license": "BSD-2-Clause",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zend-modulemanager": ">=2.2",
        "zendframework/zend-db": ">=2.2",
        "zendframework/zend-servicemanager": ">=2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "3.8.*",
        "phpunit/dbunit": ">=1.2",
        "squizlabs/php_codesniffer": "1.5.*",
        "phpmd/phpmd": "1.4.*"
    },
    "authors": [
        {
            "name": "Leonardo Di Donato",
            "email": "leodidonato@gmail.com"
        },
        {
            "name": "Leonardo Grasso",
            "email": "me@leonardograsso.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "SphinxSearch": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "keywords": [
        "zf2",
        "sphinx",
        "sphinx search",
        "search engine"
    ]
}